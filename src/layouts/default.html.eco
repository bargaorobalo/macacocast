<!doctype html>
<html lang="pt-BR">
<head>
    <title>MacacoCast - <%= @document.title %> </title>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="assets/css/bootstrap.css" rel="stylesheet">
    <link href="assets/css/bootstrap-responsive.css" rel="stylesheet">
    <link href="assets/css/default.css" rel="stylesheet">
    <link rel="shortcut icon" type="image/x-icon" href="assets/img/favicon.ico">
</head>
<body>
    <div class="container">
        <div class="row clearfix">
            <div class="column">
                <div class="logo">
                    <h1 class="text-center">
                        <img src="assets/img/monkey.png">
                    </h1>
                    <h2 class="text-center "><%= @project.name %></h2>
                    <small class="text-center"><%= @project.slogan %></small>
                </div>
            </div>
        </div>
    </div>
    <section class="container">
        <%- @content %>
    </section>


    <footer class="footer">
        <section class="all-objects">
            <img class="footer-logo" src="assets/img/monkey-monocolor.png" alt="Monkey monocolor">
            <ul class="menu">
                <li><a href="#">Home</a></li>
                <li><a href="#">Sobre</a></li>
                <li><a href="#">Feed</a></li>
                <li><a href="#">Contato</a></li>
            </ul>
            <ul class="social">
                <li class="github"><a title="Fork me on github" href="https://github.com/bargaorobalo/macacocast" target="_blank"><span>Github</span></a></li>
                <li class="twitter"><a title="Follow me on twitter" href="http://www.twitter.com/macacocast" target="_blank"><span>Twitter</span></a></li>
                <li class="facebook"><a title="Like me on facebook" href="#" target="_blank"><span>Facebook</span></a></li>
            </ul>
        </section>
    </footer>
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-46444298-1', 'bargaorobalo.github.io');
      ga('send', 'pageview');

    </script>
</body>
</html>